<?xml version="1.0" encoding="utf-8"?>
<Window xmlns:x="http://www.w3.org/2001/XMLSchema-instance" x:type="PlayerWindow">
  <IsDefault>false</IsDefault>
  <Width>1280</Width>
  <Height>720</Height>
  <Name>RadioTime</Name>
  <Id>5</Id>
  <BackgroundBrush />
  <WindowAnimations />
  <WindowControls>
    <Control x:type="Image">
      <Name>Background</Name>
      <Id>1</Id>
      <Width>1280</Width>
      <Height>720</Height>
      <VisibleCondition />
      <Animations />
      <MediaPortalFocusControls />
      <ImageStyle StyleId="Background" />
      <ImageStretch>None</ImageStretch>
      <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
    </Control>
    <Control x:type="Group">
      <Name>NowPlaying</Name>
      <Id>2</Id>
      <Width>900</Width>
      <Height>180</Height>
      <PosY>542</PosY>
      <VisibleCondition />
      <Animations>
        <Animation x:type="Slide" Condition="WindowOpen" Duration="1000" Easing="EaseOut" StartX="-900" StartY="542" EndY="542" />
        <Animation x:type="Slide" Condition="WindowClose" Duration="1000" Easing="EaseIn" StartY="542" EndX="-900" EndY="542" />
      </Animations>
      <MediaPortalFocusControls />
      <GroupStyle StyleId="Now Playing Video" />
      <GroupControls>
        <Control x:type="Equalizer">
          <Name>Equalizer</Name>
          <Id>3</Id>
          <Width>880</Width>
          <Height>160</Height>
          <PosX>10</PosX>
          <PosY>10</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <EqualizerStyle>
            <BorderThickness>0,0,0,0</BorderThickness>
            <CornerRadius>0,0,0,0</CornerRadius>
            <BackgroundBrush />
            <BorderBrush />
            <LowRangeColor x:type="GradientBrush">
              <Angle>Vertical</Angle>
              <GradientStops>
                <GradientStop Color="#50000000" Offset="1" />
                <GradientStop Color="#507DFF00" Offset="0.237" />
                <GradientStop Color="#50FFFF00" Offset="0" />
              </GradientStops>
            </LowRangeColor>
            <MedRangeColor x:type="GradientBrush">
              <Angle>Vertical</Angle>
              <GradientStops>
                <GradientStop Color="#50FFFF00" Offset="0.468" />
                <GradientStop Color="#50FF0004" Offset="0" />
              </GradientStops>
            </MedRangeColor>
            <MaxRangeColor x:type="ColorBrush" Color="#50FF0004" />
            <BandBorderColor />
            <FallOffColor x:type="ColorBrush" Color="#FF00BFFF" />
          </EqualizerStyle>
          <BandCount>25</BandCount>
          <BandSpacing>5</BandSpacing>
          <BandBorderSize>0</BandBorderSize>
          <BandCornerRadius>0</BandCornerRadius>
          <FalloffSpeed>5</FalloffSpeed>
          <FallOffHeight>2</FallOffHeight>
        </Control>
        <Control x:type="Label">
          <Name>Artist</Name>
          <Id>4</Id>
          <Width>890</Width>
          <Height>80</Height>
          <PosX>5</PosX>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="70" />
          <LabelText>#Play.Current.Title+ - +#Play.Current.Artist</LabelText>
          <LabelTextAlignment>Center</LabelTextAlignment>
          <ScrollDelay>2</ScrollDelay>
          <ScrollSpeed>10</ScrollSpeed>
          <ScrollSeperator>   |   </ScrollSeperator>
        </Control>
        <Control x:type="Label">
          <Name>CurrentPlayTime</Name>
          <Id>5</Id>
          <Width>890</Width>
          <Height>70</Height>
          <PosX>5</PosX>
          <PosY>90</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="60" />
          <LabelText>#currentplaytime</LabelText>
          <LabelTextAlignment>Center</LabelTextAlignment>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
      </GroupControls>
    </Control>
    <Control x:type="Group">
      <Name>Informations</Name>
      <Id>6</Id>
      <Width>600</Width>
      <Height>300</Height>
      <PosX>680</PosX>
      <PosY>210</PosY>
      <VisibleCondition />
      <Animations>
        <Animation x:type="Slide" Condition="WindowOpen" Duration="1000" Easing="EaseOut" StartX="1280" StartY="210" EndX="680" EndY="210" />
        <Animation x:type="Slide" Condition="WindowClose" Duration="1000" Easing="EaseIn" StartX="680" StartY="210" EndX="1280" EndY="210" />
      </Animations>
      <MediaPortalFocusControls />
      <GroupStyle StyleId="Standard Informations" />
      <GroupControls>
        <Control x:type="Label">
          <Name>Station</Name>
          <Id>7</Id>
          <Width>590</Width>
          <Height>70</Height>
          <PosX>5</PosX>
          <PosY>5</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="60" />
          <LabelText>#RadioTime.Play.Station</LabelText>
          <ScrollDelay>2</ScrollDelay>
          <ScrollSpeed>10</ScrollSpeed>
          <ScrollSeperator>   |   </ScrollSeperator>
        </Control>
        <Control x:type="Label">
          <Name>Slogan</Name>
          <Id>8</Id>
          <Width>590</Width>
          <Height>70</Height>
          <PosX>5</PosX>
          <PosY>75</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="60" />
          <LabelText>#RadioTime.Play.Slogan</LabelText>
          <ScrollDelay>2</ScrollDelay>
          <ScrollSpeed>10</ScrollSpeed>
          <ScrollSeperator>   |   </ScrollSeperator>
        </Control>
        <Control x:type="Label">
          <Name>Location</Name>
          <Id>9</Id>
          <Width>590</Width>
          <Height>70</Height>
          <PosX>5</PosX>
          <PosY>145</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="60" />
          <LabelText>#RadioTime.Play.Location</LabelText>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
        <Control x:type="Label">
          <Name>Language</Name>
          <Id>10</Id>
          <Width>590</Width>
          <Height>70</Height>
          <PosX>5</PosX>
          <PosY>215</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="60" />
          <LabelText>#RadioTime.Play.Language</LabelText>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
      </GroupControls>
    </Control>
    <Control x:type="Image">
      <Name>Logo</Name>
      <Id>11</Id>
      <Width>300</Width>
      <Height>300</Height>
      <PosX>30</PosX>
      <PosY>210</PosY>
      <VisibleCondition />
      <Animations>
        <Animation x:type="Slide" Condition="WindowOpen" Duration="1000" Easing="EaseOut" StartX="-300" StartY="210" EndX="30" EndY="210" />
        <Animation x:type="Slide" Condition="WindowClose" Duration="1000" Easing="EaseIn" StartX="30" StartY="210" EndX="-300" EndY="210" />
      </Animations>
      <MediaPortalFocusControls />
      <ImageStyle StyleId="Empty" />
      <Image>#RadioTime.Play.Image</Image>
      <DefaultImage>\Music\DefaultMyradioBig.png</DefaultImage>
      <ImageStretch>Uniform</ImageStretch>
      <CoverCornerRadius>0,0,0,0</CoverCornerRadius>
    </Control>
    <Control x:type="Group">
      <Name>Date / Time</Name>
      <Id>12</Id>
      <Width>640</Width>
      <Height>150</Height>
      <VisibleCondition />
      <Animations>
        <Animation x:type="Zoom" Condition="WindowOpen" Duration="500" Easing="EaseInOut" />
        <Animation x:type="Zoom" Condition="WindowClose" Duration="500" Easing="EaseInOut" From="100" To="0" />
        <Animation x:type="Fade" Condition="WindowClose" Duration="500" Easing="EaseInOut" From="100" To="0" />
        <Animation x:type="Fade" Condition="WindowOpen" Duration="500" Easing="EaseInOut" />
      </Animations>
      <MediaPortalFocusControls />
      <GroupStyle StyleId="DateTime" />
      <GroupControls>
        <Control x:type="Label">
          <Name>Time</Name>
          <Id>13</Id>
          <Width>625</Width>
          <Height>60</Height>
          <PosX>5</PosX>
          <PosY>5</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="50" />
          <LabelText>#time</LabelText>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
        <Control x:type="Label">
          <Name>Date</Name>
          <Id>14</Id>
          <Width>625</Width>
          <Height>60</Height>
          <PosX>5</PosX>
          <PosY>60</PosY>
          <VisibleCondition />
          <Animations />
          <MediaPortalFocusControls />
          <LabelStyle StyleId="50" />
          <LabelText>#date</LabelText>
          <IsScrollingEnabled>false</IsScrollingEnabled>
          <ScrollSpeed>2</ScrollSpeed>
          <IsScrollWrapEnabled>false</IsScrollWrapEnabled>
        </Control>
      </GroupControls>
    </Control>
  </WindowControls>
  <VisibleCondition>IsPlayer(RadioTime)</VisibleCondition>
</Window>